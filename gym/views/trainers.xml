<odoo>
    <record model="ir.actions.act_window" id="action_gym_trainers">
            <field name="name">Trainers</field>
            <field name="res_model">gym.trainers</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="view_id" eval="False"/>
    </record>
    <record model="ir.ui.view" id="gym_trainers_form">
        <field name="name">gym_trainers_form</field>
        <field name="model">gym.trainers</field>
        <field name="arch" type="xml">
            <form>
             <sheet>

                     <group>
                          <field name="name"/>
                     </group>
                     <group>
                          <field name="age"/>
                          <field name="gender"/>
                     </group>
                     <group>
                          <field name="phone"/>
                     </group>
                     <group>
                          <field name="date_employee"/>
<!--                          <field name="subscription_id"/>-->
                     </group>

             </sheet>
            </form>
        </field>
    </record>
     <record model="ir.ui.view" id="gym_trainers_tree">
        <field name="name">gym_trainers_tree</field>
        <field name="model">gym.trainers</field>
        <field name="arch" type="xml">
            <tree>
                          <field name="name"/>
                          <field name="age"/>
                          <field name="gender"/>
                          <field name="phone"/>
                          <field name="date_employee"/>
<!--                          <field name="subscription_id"/>-->
            </tree>
        </field>
    </record>
<menuitem name="Trainers" id="gym_trainers_menu" parent="gym_root_menu" action="action_gym_trainers" />
</odoo>